# ACN Configuration Example
# Copy this file to .env and update values as needed

# Service
SERVICE_NAME=ACN
SERVICE_VERSION=0.1.0
HOST=0.0.0.0
PORT=8000

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
